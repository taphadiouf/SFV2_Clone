<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>blng__TaxRuleMustBeSelected</fullName>
    <active>true</active>
    <description>Tax Rule must be selected</description>
    <errorConditionFormula>AND(OR(ISNEW(), AND (ISCHANGED(SBQQ__Activated__c), SBQQ__Activated__c)), ISBLANK(blng__TaxRule__c))</errorConditionFormula>
    <errorMessage>Tax Rule must be selected</errorMessage>
</ValidationRule>
